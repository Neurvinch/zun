# ZKVault Custom Identity Protocol - Environment Configuration
# Copy this file to .env and fill in your actual values

# Deployment Configuration
PRIVATE_KEY="your_private_key_here_without_0x_prefix"

# Blockchain RPC URLs
ETHEREUM_RPC_URL="https://eth-sepolia.g.alchemy.com/v2/your_api_key"
POLYGON_RPC_URL="https://polygon-mumbai.g.alchemy.com/v2/your_api_key"
BASE_RPC_URL="https://sepolia.base.org"
FILECOIN_CALIBRATION_RPC_URL="https://api.calibration.node.glif.io/rpc/v1"

# Mainnet RPCs (for production)
ETHEREUM_MAINNET_RPC_URL="https://eth-mainnet.g.alchemy.com/v2/your_api_key"
POLYGON_MAINNET_RPC_URL="https://polygon-mainnet.g.alchemy.com/v2/your_api_key"
BASE_MAINNET_RPC_URL="https://mainnet.base.org"
FILECOIN_MAINNET_RPC_URL="https://api.node.glif.io/rpc/v1"

# Block Explorer API Keys (for contract verification)
ETHERSCAN_API_KEY="your_etherscan_api_key"
POLYGONSCAN_API_KEY="your_polygonscan_api_key"
BASESCAN_API_KEY="your_basescan_api_key"
FILFOX_API_KEY="your_filfox_api_key"

# Storage Services
LIGHTHOUSE_API_KEY="your_lighthouse_api_key"
AKAVE_API_URL="http://localhost:8000"
AKAVE_WALLET_ADDRESS="your_akave_wallet_address"
SYNAPSE_API_KEY="your_synapse_api_key"
SYNAPSE_PRIVATE_KEY="your_synapse_private_key"

# Live Data APIs
NEWS_API_KEY="your_news_api_key"
ALPHAVANTAGE_API_KEY="your_alphavantage_api_key"
POLYGON_API_KEY="your_polygon_io_api_key"
